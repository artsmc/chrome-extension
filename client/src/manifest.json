{
  "manifest_version": 3,
  "name": "Angular QA Score ZenDesk AI",
  "short_name": "angular qa score zend",
  "version": "2.0.0",
  "description": "Angular QA Score ZenDesk AI",
  "background": {
        "service_worker": "background.js"
    },
    "action": {},
    
    "content_scripts": [
      {
        "matches": [
            "https://*/*",
            "http://*/*"
        ],
        "js": ["side-panel.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["index.html", "index.html/*"],
        "matches": ["https://*/*", "http://*/*"]
      }
    ]
}
