{
  "manifest_version": 3,
  "name": "Angular QA Score ZenDesk AI",
  "short_name": "angular qa score zend",
  "version": "2.0.0",
  "description": "Angular QA Score ZenDesk AI",
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
        "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
          "https://*/*",
          "http://*/*"
      ],
      "js": ["side-panel.js"]
    }
  ],
  
  "action": {},
  
  "web_accessible_resources": [
    {
      "resources": ["index.html", "index.html/*"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ]
}
