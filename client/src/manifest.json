{
    "manifest_version": 3,
    "name": "test",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "http://*/", "https://*/"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.ts"],
        "css": ["styles.scss"]
      }
    ]
  }
  
  