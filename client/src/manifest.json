{
  "manifest_version": 2,
  "name": "AI Email Assistant by Callcenter.ai",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "side-panel.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": ["index.html", "index.html/*"]
}
