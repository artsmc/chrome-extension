{
  "manifest_version": 3,
  "name": "AI ZenDesk Assistant by Callcentre.ai",
  "version": "1.0.2",
  "host_permissions": [
    "*://*.zendesk.com/*"
  ],
  "permissions":["tabs"],
   "icons": {
    "16": "/assets/16-mail.png",
    "32": "/assets/32-mail.png",
    "48": "/assets/48-mail.png",
    "128": "/assets/128-mail.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
            "*://*.zendesk.com/*"
        ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {},
  "web_accessible_resources":[
   {
        "resources": ["index.html", "index.html/*", "pageWorld.js", "https://s3.us-east-2.amazonaws.com/*", "/assets/16-mail.png"],
        "matches": ["https://*/*", "http://*/*"]
      }
  ]
}