{
  "manifest_version": 3,
  "name": "AI Email Assistant by Callcentre.ai",
  "version": "1.0",
  "action": {},
  "host_permissions": [
    "*://mail.google.com/*"
  ],
   "icons": {
    "16": "/assets/16-mail.png",
    "32": "/assets/32-mail.png",
    "48": "/assets/48-mail.png",
    "128": "/assets/128-mail.png"
  },
  "content_scripts": [
    {
      "matches": [
            "*://mail.google.com/*"
        ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources":[
   {
        "resources": ["index.html", "index.html/*", "pageWorld.js"],
        "matches": ["https://*/*", "http://*/*"]
      }
  ]
}