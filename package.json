{
  "name": "phoneangel-chrome-extension",
  "version": "0.5.0",
  "description": "A default scafolding for building Backend APIs with Angular 7 front-end.",
  "main": "index.js",
  "gypfile": true,
  "lint-staged": {
    "src/**/*.{ts,js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "heroku-prebuild": "npm install --dev",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "nyc mocha",
    "precommit": "lint-staged",
    "build-test": "ENV_CONFIG=testing webpack --config webpack.config.js",
    "build-prod": "ENV_CONFIG=production webpack --config webpack.config.js",
    "build": "webpack --config webpack.config.js --mode=development",
    "start": "npm-run-all --parallel watch:serve watch:client",
    "watch": "nodemon -e ts -w ./server -x npm run watch:server",
    "watch:server": "npm run build && node ./dist/server/server.js --PORT 3000",
    "watch:serve": "npm run watch",
    "watch:client": "cd ./client/ && ng build --watch "
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "mapCoverage": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,jsx}",
      "!**/node_modules/**",
      "!**/client/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "!**/webpack.config.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
